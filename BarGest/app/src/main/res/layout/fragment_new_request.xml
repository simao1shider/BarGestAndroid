<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Views.Fragments.NewRequestFragment">

    <FrameLayout
        android:id="@+id/toolbarNewRequest"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="0dp">

    </FrameLayout>

    <FrameLayout
        android:id="@+id/conteinerAddProduct"
        android:layout_width="match_parent"
        android:layout_height="260dp"
        android:layout_above="@+id/BTNCraeteRequest"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="0dp">

    </FrameLayout>

    <Button
        android:id="@+id/BTNCraeteRequest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="27dp"
        android:gravity="center|top"
        android:text="Finalizar" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/list_new_request"
        android:layout_width="match_parent"
        android:layout_height="333dp"
        android:layout_above="@+id/conteinerAddProduct"
        android:layout_below="@+id/toolbarNewRequest"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="0dp">

    </androidx.recyclerview.widget.RecyclerView>


</RelativeLayout>